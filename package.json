{
  "name": "@textileio/js-http-client",
  "version": "0.1.9",
  "description": "Official Textile JS HTTP Wrapper Client",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "credit": "node ./scripts/contributors.js",
    "test": "./node_modules/.bin/mocha --recursive",
    "test-watch": "./node_modules/.bin/mocha --reporter list --watch --recursive",
    "lint": "./node_modules/tslint/bin/tslint  -c tslint.json --project .",
    "lint-watch": "",
    "build-docs": "",
    "precommit": "yarn lint && yarn test",
    "preversion": "yarn credit; yarn precommit",
    "release": "yarn version --patch",
    "postversion": "git push --follow-tags"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 3.0.0",
    "yarn": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/textileio/js-http-client.git"
  },
  "bugs": {
    "url": "https://github.com/textileio/js-http-client/issues"
  },
  "keywords": [
    "textile",
    "photos",
    "client",
    "IPFS",
    "p2p"
  ],
  "author": "textile.io",
  "license": "MIT",
  "dependencies": {
    "@textile/go-mobile": "0.1.10-rc3",
    "axios": "^0.18.0",
    "bip39": "^2.5.0",
    "bs58": "^4.0.1",
    "crc": "^3.8.0",
    "create-hash": "^1.2.0",
    "create-hmac": "^1.1.7",
    "eventemitter2": "^5.0.1",
    "tiny-secp256k1": "^1.0.1",
    "toposort": "^2.0.2",
    "tweetnacl": "^1.0.1"
  },
  "devDependencies": {
    "@types/create-hash": "^1.2.0",
    "@types/create-hmac": "^1.1.0",
    "@types/toposort": "^2.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-strict": "^14.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "jsdoc": "^3.5.5",
    "markdown-magic": "^0.1.25",
    "markdown-magic-github-contributors": "^0.0.3",
    "mocha": "^6.0.2",
    "mocha-junit-reporter": "^1.18.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "tslint": "^5.14.0",
    "typescript": "^3.3.4000"
  }
